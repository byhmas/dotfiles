MAILDIR=${HOME}/Maildir/
DEFAULT=${HOME}/Maildir/
PMDIR=${HOME}/.procmail
LOGFILE=${MAILDIR}/procmail.log
LOG="--- Logging ${LOGFILE} for ${LOGNAME}, "
SHELL=/bin/sh
EOC=/home/hendry/opt/bin/enemies-of-carlotta

:0
* ^List-Id:.*dcglug\.org\.uk
.lists.dcglug/

:0
* ^List-Id: wp-hackers.lists.automattic.com
.lists.wp-hackers/

:0
* ^Sender: owner-mutt-users@mutt.org
.lists.mutt-users

# These rules can result in skipped mailman generated mails. Hmmm... BUG!
:0
* ^(List-Id|(X-)?Mailing-List|X-List):\/.*
{
	LISTID=$MATCH

	:0
	* LISTID ?? ^.*[<]\/[^@>\.]*
	.lists.$MATCH/

	:0
	* LISTID ?? ^\/[^@\.]*
	.lists.$MATCH/
}

:0
* ^User-Agent: rss2email
.rss/

:0
${DEFAULT}
